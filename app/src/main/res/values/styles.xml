    <item name="android:paddingLeft">12dp</item>
    <item name="android:paddingTop">12dp</item>
    <item name="android:paddingRight">12dp</item>

    </style>
    
    <style name="image1">
        <item name="android:layout_width">@dimen/imagewidth1</item>
        <item name="android:layout_height">@dimen/imageheight1</item>
        <item name="android:layout_weight">@integer/imageweight1</item>
        <item name="android:layout_marginTop">@dimen/margintop</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_marginLeft">@dimen/marginsleftandright</item>
        <item name="android:layout_marginRight">@dimen/marginsleftandright</item>
    </style>
